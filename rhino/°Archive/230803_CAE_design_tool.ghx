                <chunk name="Container">
                  <items count="13">
                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.datastructures import Network
from compas.geometry import Point
from compas_ghpython.artists import NetworkArtist, Artist

# Create a network to represent the walls of the room
room_network = Network()

# Define the vertices (nodes) for the walls
side_1 = [Point(0, 0, 0), Point(x, 0, 0)]
side_2 = [Point(x, 0, 0), Point(x, 0, y)]
side_3 = [Point(x, 0, y), Point(0, 0, y)]
side_4 = [Point(0, 0, y), Point(0, 0, 0)]


list_of_walls = [side_1, side_2, side_3, side_4]



# Add the vertices to the network
for wall in list_of_walls:
    for point in wall:
        room_network.add_node(point, attr_dict={})  # store attributes for corners

# Define the edges for the walls
wall_edges = [(side_1[0], side_1[1], {}), (side_2[0], side_2[1], {}), (side_3[0], side_3[1], {}), (side_4[0], side_4[1], {})]

# Add the edges to the network
for u, v, attr in wall_edges:
    room_network.add_edge(u, v, attr_dict=attr)

## Set attributes for the corners and openings (as an example)
