from compas.artists import Artist
from climate_active_envelopes.assembly import BRICK_Assembly as Assembly
from climate_active_envelopes.assembly import CAEElement as Element
import Rhino.Geometry as rg


# dimensions full brick
length = assembly.get_attribute('length')
width = assembly.get_attribute('width')
height = assembly.get_attribute('height')
elem_ins = assembly.get_attribute('elem_ins')


elem_full = Element.from_dimensions(length, width, height)
elem_full.tool_frame = Frame([0, 0, height], [1, 0, 0], [0, -1, 0])


elem_ins = Element.from_rhinomesh_geometry(elem_ins, Frame([0, 0, height/2], [1, 0, 0], [0, -1, 0]))
elem_ins.tool_frame = Frame([0, 0, height], [1, 0, 0], [0, -1, 0])

print elem_ins
print elem_full


