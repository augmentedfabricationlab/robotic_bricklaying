counter_key = "counter_" + guid
running_key = "running_" + guid

if counter_key not in st:
    st[counter_key] = 0

if running_key not in st:
    st[running_key] = False

if st[running_key]:
    if st[counter_key] &lt; len(L) - 1:
        st[counter_key] += 1
        update_component(ghenv, interval)
    else:
        st[counter_key] = 0
        st[running_key] = False

if step:
