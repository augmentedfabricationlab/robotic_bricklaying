                    <item name="CodeInput" type_name="gh_string" type_code="10">    def create_outer_wall_network(self, length, width, height):
        """Create network from mesh
        """
        mesh_wall = Mesh()
        # Add front side vertices
        for j in range(2):
            x = j * width
            for i in range(2):
                z = i * height
                mesh_wall.add_vertex(x=x, y=0, z=z)
        
        # Add front side face
        for j in range(1):
            for i in range(1):
                a = i + j * 2
                b = a + 1
                c = b + 2
                d = a + 2
                mesh_wall.add_face([a, b, c, d])
        
        # add left side vertices
        for j in range(2):
            z = j * height
            for i in range(2):
                y = i * length
                x = 0
                mesh_wall.add_vertex(x=x, y=y, z=z)
        
        # add left side face
        for j in range(1):
            for i in range(1):
                a = 4 + i + j * 2
                b = a + 1
                c = b + 2
                d = a + 2
                mesh_wall.add_face([a, b, c, d])
        
        # add right side vertices
        for j in range(2):
            z = j * height
            for i in range(2):
                y = i * length
                x = width
                mesh_wall.add_vertex(x=x, y=y, z=z)
        
        # add right side face
        for j in range(1):
            for i in range(1):
                a = 8 + i + j * 2
                b = a + 1
                c = b + 2
                d = a + 2
                mesh_wall.add_face([a, b, c, d])
        
        # Add back side vertices
        for j in range(2):
            x = j * width
            for i in range(2):
                y = length
                z = i * height
                mesh_wall.add_vertex(x=x, y=y, z=z)
        
        # Add back side face
        for j in range(1):
            for i in range(1):
                a = 12 + i + j * 2
                b = a + 1
                c = b + 2
                d = a + 2
                mesh_wall.add_face([a, b, c, d])

        network = self.network
        centroids = []
        
        # add face centroids as node to the network
        for face in mesh_wall.faces():
            center = mesh_wall.face_centroid(face)
            centroids.append(center)
        
        for i, center_pts in enumerate(centroids):
            network.add_node(i, x=center_pts[0], y=center_pts[1], z=center_pts[2])
        
        # add face edges of adjoining faces to the network
        connections = [(0, [1, 2]), (1, [0, 3]), (2, [0, 3]), (3, [1, 2])]
        
        for node, neighbors in connections:
            for neighbor in neighbors:
                # check both directions of the nodes if edge already exists
                if network.has_edge(node, neighbor) != True:
                    if network.has_edge(neighbor, node) != True:
                        network.add_edge(node, neighbor)

        return network</item>
