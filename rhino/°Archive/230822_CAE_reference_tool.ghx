#width = assembly.get_attribute('width')

curves = []
global_plane_grid = []

mortar = 0.005
#length = length + 0.06
n = num_row



def create_global_planes(curve, num_row):
    for i in range(n):
        crv = curve.ToNurbsCurve()
        crv.Translate(rg.Vector3d.ZAxis*(height+mortar)*i)
        curves.append(crv)


    for j, curve in enumerate(curves):
        row = []
        vparams = rg.Curve.DivideByLength(curve, (length+mortar), True)
        planes = rg.Curve.GetPerpendicularFrames(curve, vparams)

        for k, plane in enumerate(planes):
            plane.Rotate(m.radians(-90), plane.YAxis)
            plane.Translate(plane.XAxis*+(width+mortar/2))
            row.append(plane)

        global_plane_grid.append(row)
    return global_plane_grid

global_plane_grid = create_global_planes(curve, num_row)


def list_from_2darray(array):
    return [item for sublist in array for item in sublist]

planes = list_from_2darray(global_plane_grid)



crv_loft = rg.Brep.CreateFromLoft(curves, rg.Point3d.Unset, rg.Point3d.Unset, rg.LoftType(2), False )
print crv_loft</item>
