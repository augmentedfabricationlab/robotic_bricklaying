meshes = []
vertices = []
labels = []

if show_faces:
    M = MeshArtist(mesh_wall).draw()
    #M_left = MeshArtist(mesh_wall_left).draw()
    #M_right = MeshArtist(mesh_wall_right).draw()

if show_vertices:
    V = MeshArtist(mesh_wall).draw_vertices()
    #V_left = MeshArtist(mesh_wall_left).draw_vertices()
    #V_right = MeshArtist(mesh_wall_right).draw_vertices()

if show_labels:
    L = [key for key in mesh_wall.vertices()]
    #L_left = [key for key in mesh_wall_left.vertices()]
    #L_right = [key for key in mesh_wall_right.vertices()]


    meshes = M

