import math as m
from compas.geometry import Curve

length = assembly.get_attribute('length')
height = assembly.get_attribute('height')
width = assembly.get_attribute('width')

curves = []
plane_grid = []

mortar = 0.005

def create_planes_plane_grid(height, width):

    for i, planes in enumerate(global_plane_grid):
        row = []
        for j, plane in enumerate(planes):
            #print len(plane)
            z1 = plane

            #zone 1

            #z1.Translate(z1.ZAxis*-height)
            if i %2 == 0:
                z1.Translate(z1.XAxis*-(length/2 + mortar/2))
                
                if j == 0: #first planes
                    z1.Rotate(m.radians(90), plane.ZAxis)
                    z1.Translate(z1.XAxis*-(width/2 + mortar))
                    z1.Translate(z1.YAxis*-(width/2 + mortar))
                    
                if j == len(planes)-1: #last planes
                    z1_copy = rg.Plane(z1)
                    z1_copy.Rotate(m.radians(90), z1_copy.ZAxis)
                    z1_copy.Translate(z1_copy.YAxis*-(width+width/2+mortar*2))
                    z1_copy.Translate(z1_copy.XAxis*-(width/2 + mortar))
                    row.append(z1_copy)

            row.append(z1)


        plane_grid.append(row)
    return plane_grid

plane_grid = create_planes_plane_grid(height, width)





def list_from_2darray(array):
    return [item for sublist in array for item in sublist]

planes = list_from_2darray(plane_grid)

