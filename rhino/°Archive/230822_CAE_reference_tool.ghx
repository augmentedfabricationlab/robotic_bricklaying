                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
import sys

import math

from compas.colors import Color
from compas.artists import Artist
from compas_ghpython.artists import NetworkArtist


ref_elems = []
ref_elem = []

key_index = reference_model.network.key_index()
keys = [key_index[key] for key in reference_model.network.nodes()]
elements = [reference_model.element(key) for key in keys]

planes = [Artist(e.frame).draw() for e in elements]
current_plane = planes[counter]


if show_all:
    for element in elements:
        mesh = Artist(element.mesh).draw(Color.from_rgb255(220,165,165))
        ref_elems.append(mesh)


if show_current:
    element = reference_model.element(counter)
    mesh = Artist(element.mesh).draw(Color.from_rgb255(41,49,51))
    ref_elem.append(mesh)
