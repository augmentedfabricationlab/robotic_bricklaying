                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
from compas.utilities import normalize_values
from compas.geometry import Curve, Point, Frame, Vector
from compas.artists import Artist



def calculate_distance_to(transf_values, mesh_wall):
    #normalize values from the simulation
    transf_values = normalize_values(transf_values, 0.00, -0.3)

    # create list of face_centroids of the initial mesh
    c_pts = []
    for j, face in enumerate(mesh_wall.faces()):
        vertex_centroid = mesh_wall.face_centroid(face)
        points = rg.Point3d(vertex_centroid[0],vertex_centroid[1], vertex_centroid[2])
        c_pts.append(points)

    #create new planes / points from transf_values
    t_pts = []
    t_pls = []
    for i, points in enumerate(c_pts):
        x = points[0]
        z = points[2]
        y = transf_values[i]
        #create transformation points
        t_pt = rg.Point3d(x,y,z)
        t_pts.append(t_pt)
    
        # create planes from points
        t_pl = rg.Plane(rg.Point3d(x,y,z), rg.Vector3d(1, 0, 0), rg.Vector3d(0, 1, 0))
        t_pls.append(t_pl)

    #t_points = []
    distances = []
    for c_pt, t_pt in zip(c_pts, t_pts):
        c_point = rg.Point3d(c_pt)
        t_point = rg.Point3d(t_pt)
        #t_points.append(t_point)
        distance = rg.Point3d.DistanceTo(c_point, t_point)
        distances.append(distance)
    return distances
    #return t_points

