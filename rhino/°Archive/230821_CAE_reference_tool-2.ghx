                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Frame, Vector
from climate_active_envelopes.assembly import ReferenceElement as Element
from climate_active_envelopes.assembly import ReferenceModel as Assembly
from compas.geometry import Frame, Point, Rotation, Transformation, Translation

frame = Frame.worldXY()

front_elem = Element.create_ref_elem(frame, length, height)
back_element = Element.create_ref_elem(frame, length, height)
right_element = Element.create_ref_elem(frame, length+offset, height)
left_element = Element.create_ref_elem(frame, length+offset, height)

back_frame = Frame(Point(length, length+offset, 0), Vector(-1, 0, 0), Vector(0, -1, 0))
right_frame = Frame(Point(length, 0, 0), Vector(0, 1, 0), Vector(-1, 0, 0))
left_frame = Frame(Point(0, length+offset, 0), Vector(0, -1, 0), Vector(1, 0 , 0))


reference_model = Assembly()

T1 = Transformation.from_frame_to_frame(frame, back_frame)
T2 = Transformation.from_frame_to_frame(frame, right_frame)
T3 =Transformation.from_frame_to_frame(frame, left_frame)

back_elem = back_element.transformed(T1)
right_elem = right_element.transformed(T2)
left_elem = left_element.transformed(T3)

reference_model.add_element(front_elem)
reference_model.add_element(back_elem)
reference_model.add_element(right_elem)
reference_model.add_element(left_elem)

reference_model.add_connection(0, 2)
reference_model.add_connection(1, 3)
reference_model.add_connection(2, 1)
reference_model.add_connection(3, 0)
print reference_model

"""
