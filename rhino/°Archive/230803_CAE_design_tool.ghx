                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.datastructures import Network
from compas.geometry import Line
from compas.artists import NetworkArtist, Artist

# Create a Network instance
network = Network()

# Define the four sides of the rectangular room as Line segments
sides = [
    Line((0, 0, 0), (5, 0, 0)),
    Line((5, 0, 0), (5, 3, 0)),
    Line((5, 3, 0), (0, 3, 0)),
    Line((0, 3, 0), (0, 0, 0))
]



# Add the sides as nodes to the network
for i, side in enumerate(sides):
    network.add_node(i, x=side.midpoint[0], y=side.midpoint[1], z=side.midpoint[2])

# Define the edges between the nodes to represent the connections between sides
edges = [
    (0, 1),
    (1, 2),
    (2, 3),
    (3, 0)
]

# Add the edges to the network
for u, v in edges:
    network.add_edge(u, v)

# Create a NetworkArtist instance to visualize the network
edges, nodes = NetworkArtist(network).draw()</item>
