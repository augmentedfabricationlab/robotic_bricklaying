                  <items count="14">
                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
from compas.utilities import normalize_values
from compas.geometry import Curve, Point, Frame


# create list of face_centroids
c_pts = []
for j, face in enumerate(mesh_wall.faces()):
    vertex_centroid = mesh_wall.face_centroid(face)
    points = rg.Point3d(vertex_centroid[0],vertex_centroid[1], vertex_centroid[2])
    c_pts.append(points)

#normalize values from the simulation
transf_values = normalize_values(transf_values, -1.0, 0.0)
params_grid = []

# create new planes / points from transf_values
t_pts = []
t_pls = []
for i, points in enumerate(c_pts):
    x = points[0]
    z = points[2]
    y = transf_values[i]
    t_pt = rg.Point3d(x,y,z)
